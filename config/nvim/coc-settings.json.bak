{
  "coc.preferences.formatOnSaveFiletypes": ["rust"],
  "diagnostic.messageTarget": "float",
  "css.lint.unknownAtRules": "ignore",
  "signature.target": "float",
  "snippets.priority": 100,
  "snippets.snipmate.enable": false,
  "suggest.floatEnable": true,
  "suggest.echodocSupport": false,
  "suggest.noselect":false,
  "suggest.minTriggerInputLength": 2,
  "suggest.preferCompleteThanJumpPlaceholder":true,
  "suggest.acceptSuggestionOnCommitCharacter": true,
  "suggest.snippetIndicator": "â–º",
  "suggest.maxCompleteItemCount": 30,
  "rust.rustfmt_path": "~/.cargo/bin/rustfmt",
  "rust.full_docs": true,
  "rust.clippy_preference": "on",
  "rust.all_features": true,
  "vetur.experimental.templateInterpolationService": false,
  "vetur.validation.template": false,
  "list.insertMappings": {
    "<C-n>": "normal:j",
    "<C-p>": "normal:k",
    "<C-f>": "prompt:left",
    "<C-b>": "prompt:right"
  },
  "yaml.schemas": {
    "https://raw.githubusercontent.com/awslabs/goformation/master/schema/cloudformation.schema.json": "/cloudformation/*.yml"
  },
  "yaml.customTags": [
    "!Ref",
    "!Sub scalar",
    "!Sub sequence",
    "!Join sequence",
    "!FindInMap sequence",
    "!GetAtt scalar",
    "!GetAtt sequence",
    "!Base64 mapping",
    "!GetAZs",
    "!Select scalar",
    "!Select sequence",
    "!Split sequence",
    "!ImportValue",
    "!Condition",
    "!Equals sequence",
    "!And",
    "!If",
    "!Not",
    "!Or"
  ],
  "diagnostic-languageserver.filetypes": {
    "php": ["phpstan"],
    "sh": "shellcheck",
    "bash": "shellcheck",
    "zsh": "shellcheck"
  },
  "diagnostic-languageserver.formatFiletypes": {
    "sh": "shfmt",
    "bash": "shfmt",
    "zsh": "shfmt"
  },
  "diagnostic-languageserver.mergeConfig": true,
  "diagnostic-languageserver.linters": {
    "phpstan": {
      "command": "~/.config/composer/vendor/bin/phpstan"
    }
  },

  "languageserver": {
    "yaml": {
      "command": "yaml-language-server",
      "args": ["--stdio"],
      "filetypes": ["yaml", "yml"]
    },
    "intelephense": {
      "command": "intelephense",
      "args": ["--stdio"],
      "filetypes": ["php"],
      "initializationOptions": {
        "storagePath": "/tmp/intelephense"
      }
    }
  }
}
